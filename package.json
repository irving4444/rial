{
  "name": "zk-img",
  "version": "1.0.0",
  "description": "Zero-Knowledge Image Authentication System - Prove photos are REAL and happened in REAL LIFE",
  "main": "index.js",
  "scripts": {
    "setup:all": "npm run setup-backend && npm run setup-ios",
    "setup-backend": "cd backend && npm install",
    "setup-ios": "echo 'iOS dependencies handled by Xcode'",
    "setup-zk": "cd backend && npm run setup-zk",
    "dev": "npm run dev-backend",
    "dev-backend": "cd backend && npm start",
    "test": "cd backend && npm run test-authenticity",
    "test-authenticity": "cd backend && node test-authenticity.js",
    "demo": "node demo-all-features.js",
    "benchmark": "cd backend && npm run benchmark",
    "setup:all": "npm run setup-backend && echo 'iOS setup: Open rial/rial/rial.xcodeproj in Xcode'",
    "setup-backend": "cd backend && npm install",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "clean": "cd backend && npm run clean",
    "build-ios": "echo 'Open rial/rial/rial.xcodeproj in Xcode to build'"
  },
  "keywords": [
    "zero-knowledge",
    "zkp",
    "cryptography",
    "image-authentication",
    "anti-ai",
    "blockchain",
    "fraud-detection",
    "circom",
    "halo2",
    "groth16"
  ],
